<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="3"
      main_tree_to_execute="floor_mission">

  <BehaviorTree ID="floor_mission">
    <Sequence>
      <!-- <CheckFloorMissionTriggered/> -->

      <RetryUntilSuccessful num_attempts="-1">
        <Delay delay_msec="2">
          <CheckIdle/>
        </Delay>
      </RetryUntilSuccessful>

      <GoToPlace vertex_name="worker_misha_pickup_point"/>

      <!-- TODO - add method of checking if go to place method actually started getting executed. -->
      <RetryUntilSuccessful num_attempts="-1">
        <Delay delay_msec="2">
          <CheckIdle/>
        </Delay>
      </RetryUntilSuccessful>


      <SuspendRMFPathing/>
      <PickupWorker/>
      <ReleaseRMFPathing/>

      <GoToPlace vertex_name="S2_L_R1"/>
      <!-- TODO - add method of checking if go to place method actually started getting executed. -->
      <RetryUntilSuccessful num_attempts="-1">
        <Delay delay_msec="2">
          <CheckIdle/>
        </Delay>
      </RetryUntilSuccessful>

      <SuspendRMFPathing/>
      <LowerWorker/>

      <GoToPlace vertex_name="S2_L_R1_pickup_point"/>
      <!-- TODO - add method of checking if go to place method actually started getting executed. -->
      <RetryUntilSuccessful num_attempts="-1">
        <Delay delay_msec="2">
          <CheckIdle/>
        </Delay>
      </RetryUntilSuccessful>

      <ReleaseRMFPathing/>




    </Sequence>

  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
  </TreeNodesModel>

</root>
